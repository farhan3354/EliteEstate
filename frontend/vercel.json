{
    "routes": [
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|json|txt|xml))",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/user-dashboard",
            "dest": "/index.html"
        },
        {
            "src": "/user-dashboard/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}